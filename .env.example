# AgentFlow Environment Configuration
# Copy to .env and customize. Scripts source this automatically.
# Precedence: command-line flags > env vars > .env > defaults

# MongoDB
MONGODB_PORT=27018
AFL_MONGODB_URL=mongodb://localhost:27018
AFL_MONGODB_DATABASE=afl
# MONGODB_DATA_DIR=

# Scaling
AFL_RUNNERS=1
AFL_AGENTS=1
AFL_OSM_AGENTS=0
AFL_OSM_LITE_AGENTS=0

# Runner tuning
# AFL_MAX_CONCURRENT=2
# AFL_POLL_INTERVAL_MS=1000

# Overlays
AFL_HDFS=false
AFL_POSTGIS=false
AFL_JENKINS=false
# AFL_GEOFABRIK_MIRROR=

# External data dirs (leave empty for Docker volumes)
# HDFS_NAMENODE_DIR=
# HDFS_DATANODE_DIR=
# GRAPHHOPPER_DATA_DIR=
# POSTGIS_DATA_DIR=
# JENKINS_HOME_DIR=
