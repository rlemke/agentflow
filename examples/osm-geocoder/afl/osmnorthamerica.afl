namespace osm.geo.NorthAmerica.cache {
// NorthAmerica
  use osm.types
  use osm.geo.cache.NorthAmerica


  facet NorthAmericaIndividually () => (cache: [OSMCache]) andThen {

    canada = Canada()
    mexico = Mexico()
    unitedStates = UnitedStates()
    greenland = Greenland()

    yield NorthAmericaIndividually(cache = canada.cache ++
                                   mexico.cache ++
                                   unitedStates.cache ++
                                   greenland.cache)

  }

}
