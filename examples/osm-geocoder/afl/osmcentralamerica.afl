/** Aggregates individual Central American and Caribbean country OSM data downloads. */
namespace osm.geo.CentralAmerica.cache {
  use osm.types
  use osm.geo.cache.CentralAmerica

  /** Caches each Central American and Caribbean country individually in parallel. */
  facet CentralAmericaIndividually () => (cache: [OSMCache]) andThen {

    bahamas = Bahamas()
    belize = Belize()
    costaRica = CostaRica()
    cuba = Cuba()
    elSalvador = ElSalvador()
    dominicanRepublic = DominicanRepublic()
    guatemala = Guatemala()
    haiti = Haiti()
    honduras = Honduras()
    jamaica = Jamaica()
    nicaragua = Nicaragua()
    panama = Panama()

    yield CentralAmericaIndividually( cache =
      bahamas.cache ++
      belize.cache ++
      costaRica.cache ++
      cuba.cache ++
      elSalvador.cache ++
      dominicanRepublic.cache ++
      guatemala.cache ++
      haiti.cache ++
      honduras.cache ++
      jamaica.cache ++
      nicaragua.cache ++
      panama.cache)
   }
}
