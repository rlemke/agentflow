/** Aggregates individual Australia/Oceania region OSM data downloads. */
namespace osm.geo.Australia.cache {
  use osm.types
  use osm.geo.cache.Australia

  /** Caches each Australia/Oceania region individually in parallel. */
  facet AustraliaIndividually() => (cache: [OSMCache]) andThen {

    americanOceania = osm.geo.cache.Australia.AmericanOceania()
    cookIslands = osm.geo.cache.Australia.CookIslands()
    fiji = osm.geo.cache.Australia.Fiji()
    kiribati = osm.geo.cache.Australia.Kiribati()
    ileDeClipperton = osm.geo.cache.Australia.IleDeClipperton()
    marshallIslands = osm.geo.cache.Australia.MarshallIslands()
    micronesia = osm.geo.cache.Australia.Micronesia()
    nauru = osm.geo.cache.Australia.Nauru()
    newCaledonia = osm.geo.cache.Australia.NewCaledonia()
    newZealand = osm.geo.cache.Australia.NewZealand()
    palau = osm.geo.cache.Australia.Palau()
    papuaNewGuinea = osm.geo.cache.Australia.PapuaNewGuinea()
    samoa = osm.geo.cache.Australia.Samoa()
    solomonIslands = osm.geo.cache.Australia.SolomonIslands()
    tokelau = osm.geo.cache.Australia.Tokelau()
    tonga = osm.geo.cache.Australia.Tonga()
    tuvalu = osm.geo.cache.Australia.Tuvalu()
    vanuatu = osm.geo.cache.Australia.Vanuatu()
    wallisAndFutuna = osm.geo.cache.Australia.WallisAndFutuna()

    yield AustraliaIndividually(cache = americanOceania.cache ++
            cookIslands.cache ++
            fiji.cache ++
            kiribati.cache ++
            ileDeClipperton.cache ++
            marshallIslands.cache ++
            micronesia.cache ++
            nauru.cache ++
            newCaledonia.cache ++
            newZealand.cache ++
            palau.cache ++
            papuaNewGuinea.cache ++
            samoa.cache ++
            solomonIslands.cache ++
            tokelau.cache ++
            tonga.cache ++
            tuvalu.cache ++
            vanuatu.cache ++
            wallisAndFutuna.cache)
  }

}
