/** Aggregates individual Asian country OSM data downloads. */
namespace osm.geo.Asia.cache {
  use osm.types
  use osm.geo.cache.Asia

  /** Caches each Asian country individually in parallel. */
  facet AsiaIndividually() => (cache: [OSMCache]) andThen {

    afghanistan = Afghanistan()
    armenia = Armenia()
    azerbaijan = Azerbaijan()
    bangladesh = Bangladesh()
    bhutan = Bhutan()
    brunei = Brunei()
    cambodia = Cambodia()
    china = China()
    eastTimor = EastTimor()
    gCCStates = GCCStates()
    india = India()
    indonesia = Indonesia()
    iran = Iran()
    iraq = Iraq()
    japan = Japan()
    jordan = Jordan()
    kazakhstan = Kazakhstan()
    kyrgyzstan = Kyrgyzstan()
    laos = Laos()
    lebanon = Lebanon()
    malaysia = Malaysia()
    maldives = Maldives()
    mongolia = Mongolia()
    myanmar = Myanmar()
    nepal = Nepal()
    northKorea = NorthKorea()
    pakistan = Pakistan()
    israelAndPalestine = IsraelAndPalestine()
    philippines = Philippines()
    southKorea = SouthKorea()
    sriLanka = SriLanka()
    syria = Syria()
    tajikistan = Tajikistan()
    thailand = Thailand()
    turkmenistan = Turkmenistan()
    uzbekistan = Uzbekistan()
    vietnam = Vietnam()
    yemen = Yemen()

    yield AsiaIndividually(cache = afghanistan.cache ++
                                   armenia.cache ++
                                   azerbaijan.cache ++
                                   bangladesh.cache ++
                                   bhutan.cache ++
                                   brunei.cache ++
                                   cambodia.cache ++
                                   china.cache ++
                                   eastTimor.cache ++
                                   gCCStates.cache ++
                                   india.cache ++
                                   indonesia.cache ++
                                   iran.cache ++
                                   iraq.cache ++
                                   japan.cache ++
                                   jordan.cache ++
                                   kazakhstan.cache ++
                                   kyrgyzstan.cache ++
                                   laos.cache ++
                                   lebanon.cache ++
                                   malaysia.cache ++
                                   maldives.cache ++
                                   mongolia.cache ++
                                   myanmar.cache ++
                                   nepal.cache ++
                                   northKorea.cache ++
                                   pakistan.cache ++
                                   israelAndPalestine.cache ++
                                   philippines.cache ++
                                   southKorea.cache ++
                                   sriLanka.cache ++
                                   syria.cache ++
                                   tajikistan.cache ++
                                   thailand.cache ++
                                   turkmenistan.cache ++
                                   uzbekistan.cache ++
                                   vietnam.cache ++
                                   yemen.cache)

  }

}
