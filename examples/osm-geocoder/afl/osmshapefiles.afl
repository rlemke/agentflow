    slovenia = Slovenia()
    spain = Spain()
    sweden = Sweden()
    switzerland = Switzerland()
    turkey = Turkey()
    ukraine = Ukraine()
    unitedKingdom = UnitedKingdom()

    shp_albania = DownloadShapefile(cache = albania.cache)
    shp_andorra = DownloadShapefile(cache = andorra.cache)
    shp_austria = DownloadShapefile(cache = austria.cache)
    shp_belarus = DownloadShapefile(cache = belarus.cache)
    shp_belgium = DownloadShapefile(cache = belgium.cache)
