    shp_spain = DownloadShapefile(cache = spain.cache)
    shp_sweden = DownloadShapefile(cache = sweden.cache)
    shp_switzerland = DownloadShapefile(cache = switzerland.cache)
    shp_turkey = DownloadShapefile(cache = turkey.cache)
    shp_ukraine = DownloadShapefile(cache = ukraine.cache)
    shp_unitedKingdom = DownloadShapefile(cache = unitedKingdom.cache)

    yield EuropeShapefiles(cache = shp_albania.cache ++
            shp_andorra.cache ++
            shp_austria.cache ++
            shp_belarus.cache ++
            shp_belgium.cache ++
