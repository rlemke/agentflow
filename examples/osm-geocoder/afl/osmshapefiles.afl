    shp_bulgaria = DownloadShapefile(cache = bulgaria.cache)
    shp_croatia = DownloadShapefile(cache = croatia.cache)
    shp_cyprus = DownloadShapefile(cache = cyprus.cache)
    shp_czechRepublic = DownloadShapefile(cache = czechRepublic.cache)
    shp_denmark = DownloadShapefile(cache = denmark.cache)
    shp_estonia = DownloadShapefile(cache = estonia.cache)
    shp_finland = DownloadShapefile(cache = finland.cache)
    shp_france = DownloadShapefile(cache = france.cache)
    shp_germany = DownloadShapefile(cache = germany.cache)
    shp_greece = DownloadShapefile(cache = greece.cache)
    shp_georgia = DownloadShapefile(cache = georgia.cache)
    shp_hungary = DownloadShapefile(cache = hungary.cache)
    shp_iceland = DownloadShapefile(cache = iceland.cache)
    shp_ireland = DownloadShapefile(cache = ireland.cache)
    shp_italy = DownloadShapefile(cache = italy.cache)
    shp_kosovo = DownloadShapefile(cache = kosovo.cache)
    shp_latvia = DownloadShapefile(cache = latvia.cache)
    shp_liechtenstein = DownloadShapefile(cache = liechtenstein.cache)
    shp_lithuania = DownloadShapefile(cache = lithuania.cache)
    shp_luxembourg = DownloadShapefile(cache = luxembourg.cache)
    shp_macedonia = DownloadShapefile(cache = macedonia.cache)
    shp_malta = DownloadShapefile(cache = malta.cache)
