/** Aggregates individual US state OSM data downloads. */
namespace osm.geo.UnitedStates.cache {
  use osm.types
  use osm.geo.cache.UnitedStates

  /** Caches each US state individually in parallel. */
  facet UnitedStatesIndividually () => (cache: [OSMCache]) andThen {

    alabama = osm.geo.cache.UnitedStates.Alabama()
    alaska = osm.geo.cache.UnitedStates.Alaska()
    arizona = osm.geo.cache.UnitedStates.Arizona()
    arkansas = osm.geo.cache.UnitedStates.Arkansas()
    california = osm.geo.cache.UnitedStates.California()
    colorado = osm.geo.cache.UnitedStates.Colorado()
    connecticut = osm.geo.cache.UnitedStates.Connecticut()
    delaware = osm.geo.cache.UnitedStates.Delaware()
    districtOfColumbia = osm.geo.cache.UnitedStates.DistrictOfColumbia()
    florida = osm.geo.cache.UnitedStates.Florida()
    georgia = osm.geo.cache.UnitedStates.Georgia()
    hawaii = osm.geo.cache.UnitedStates.Hawaii()
    idaho = osm.geo.cache.UnitedStates.Idaho()
    illinois = osm.geo.cache.UnitedStates.Illinois()
    indiana = osm.geo.cache.UnitedStates.Indiana()
    iowa = osm.geo.cache.UnitedStates.Iowa()
    kansas = osm.geo.cache.UnitedStates.Kansas()
    kentucky = osm.geo.cache.UnitedStates.Kentucky()
    louisiana = osm.geo.cache.UnitedStates.Louisiana()
    maine = osm.geo.cache.UnitedStates.Maine()
    maryland = osm.geo.cache.UnitedStates.Maryland()
    massachusetts = osm.geo.cache.UnitedStates.Massachusetts()
    michigan = osm.geo.cache.UnitedStates.Michigan()
    minnesota = osm.geo.cache.UnitedStates.Minnesota()
    mississippi = osm.geo.cache.UnitedStates.Mississippi()
    missouri = osm.geo.cache.UnitedStates.Missouri()
    montana = osm.geo.cache.UnitedStates.Montana()
    nebraska = osm.geo.cache.UnitedStates.Nebraska()
    nevada = osm.geo.cache.UnitedStates.Nevada()
    newHampshire = osm.geo.cache.UnitedStates.NewHampshire()
    newJersey = osm.geo.cache.UnitedStates.NewJersey()
    newMexico = osm.geo.cache.UnitedStates.NewMexico()
    newYork = osm.geo.cache.UnitedStates.NewYork()
    northCarolina = osm.geo.cache.UnitedStates.NorthCarolina()
    northDakota = osm.geo.cache.UnitedStates.NorthDakota()
    ohio = osm.geo.cache.UnitedStates.Ohio()
    oklahoma = osm.geo.cache.UnitedStates.Oklahoma()
    oregon = osm.geo.cache.UnitedStates.Oregon()
    pennsylvania = osm.geo.cache.UnitedStates.Pennsylvania()
    rhodeIsland = osm.geo.cache.UnitedStates.RhodeIsland()
    southCarolina = osm.geo.cache.UnitedStates.SouthCarolina()
    southDakota = osm.geo.cache.UnitedStates.SouthDakota()
    tennessee = osm.geo.cache.UnitedStates.Tennessee()
    texas = osm.geo.cache.UnitedStates.Texas()
    utah = osm.geo.cache.UnitedStates.Utah()
    vermont = osm.geo.cache.UnitedStates.Vermont()
    virginia = osm.geo.cache.UnitedStates.Virginia()
    washington = osm.geo.cache.UnitedStates.Washington()
    westVirginia = osm.geo.cache.UnitedStates.WestVirginia()
    wisconsin = osm.geo.cache.UnitedStates.Wisconsin()
    wyoming = osm.geo.cache.UnitedStates.Wyoming()

    yield UnitedStatesIndividually(cache = alabama.cache ++
                                   alaska.cache ++
                                   arizona.cache ++
                                   arkansas.cache ++
                                   california.cache ++
                                   colorado.cache ++
                                   connecticut.cache ++
                                   delaware.cache ++
                                   districtOfColumbia.cache ++
                                   florida.cache ++
                                   georgia.cache ++
                                   hawaii.cache ++
                                   idaho.cache ++
                                   illinois.cache ++
                                   indiana.cache ++
                                   iowa.cache ++
                                   kansas.cache ++
                                   kentucky.cache ++
                                   louisiana.cache ++
                                   maine.cache ++
                                   maryland.cache ++
                                   massachusetts.cache ++
                                   michigan.cache ++
                                   minnesota.cache ++
                                   mississippi.cache ++
                                   missouri.cache ++
                                   montana.cache ++
                                   nebraska.cache ++
                                   nevada.cache ++
                                   newHampshire.cache ++
                                   newJersey.cache ++
                                   newMexico.cache ++
                                   newYork.cache ++
                                   northCarolina.cache ++
                                   northDakota.cache ++
                                   ohio.cache ++
                                   oklahoma.cache ++
                                   oregon.cache ++
                                   pennsylvania.cache ++
                                   rhodeIsland.cache ++
                                   southCarolina.cache ++
                                   southDakota.cache ++
                                   tennessee.cache ++
                                   texas.cache ++
                                   utah.cache ++
                                   vermont.cache ++
                                   virginia.cache ++
                                   washington.cache ++
                                   westVirginia.cache ++
                                   wisconsin.cache ++
                                   wyoming.cache)

  }

}
