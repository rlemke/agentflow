/** Aggregates individual Asian country OSM data downloads. */
namespace osm.geo.Asia.cache {
  use osm.types
  use osm.geo.cache.Asia

  /** Caches each Asian country individually in parallel. */
  facet AsiaIndividually() => (cache: [OSMCache]) andThen {

    afghanistan = osm.geo.cache.Asia.Afghanistan()
    armenia = osm.geo.cache.Asia.Armenia()
    azerbaijan = osm.geo.cache.Asia.Azerbaijan()
    bangladesh = osm.geo.cache.Asia.Bangladesh()
    bhutan = osm.geo.cache.Asia.Bhutan()
    brunei = osm.geo.cache.Asia.Brunei()
    cambodia = osm.geo.cache.Asia.Cambodia()
    china = osm.geo.cache.Asia.China()
    eastTimor = osm.geo.cache.Asia.EastTimor()
    gCCStates = osm.geo.cache.Asia.GCCStates()
    india = osm.geo.cache.Asia.India()
    indonesia = osm.geo.cache.Asia.Indonesia()
    iran = osm.geo.cache.Asia.Iran()
    iraq = osm.geo.cache.Asia.Iraq()
    japan = osm.geo.cache.Asia.Japan()
    jordan = osm.geo.cache.Asia.Jordan()
    kazakhstan = osm.geo.cache.Asia.Kazakhstan()
    kyrgyzstan = osm.geo.cache.Asia.Kyrgyzstan()
    laos = osm.geo.cache.Asia.Laos()
    lebanon = osm.geo.cache.Asia.Lebanon()
    malaysia = osm.geo.cache.Asia.Malaysia()
    maldives = osm.geo.cache.Asia.Maldives()
    mongolia = osm.geo.cache.Asia.Mongolia()
    myanmar = osm.geo.cache.Asia.Myanmar()
    nepal = osm.geo.cache.Asia.Nepal()
    northKorea = osm.geo.cache.Asia.NorthKorea()
    pakistan = osm.geo.cache.Asia.Pakistan()
    israelAndPalestine = osm.geo.cache.Asia.IsraelAndPalestine()
    philippines = osm.geo.cache.Asia.Philippines()
    southKorea = osm.geo.cache.Asia.SouthKorea()
    sriLanka = osm.geo.cache.Asia.SriLanka()
    syria = osm.geo.cache.Asia.Syria()
    tajikistan = osm.geo.cache.Asia.Tajikistan()
    thailand = osm.geo.cache.Asia.Thailand()
    turkmenistan = osm.geo.cache.Asia.Turkmenistan()
    uzbekistan = osm.geo.cache.Asia.Uzbekistan()
    vietnam = osm.geo.cache.Asia.Vietnam()
    yemen = osm.geo.cache.Asia.Yemen()

    yield AsiaIndividually(cache = afghanistan.cache ++
                                   armenia.cache ++
                                   azerbaijan.cache ++
                                   bangladesh.cache ++
                                   bhutan.cache ++
                                   brunei.cache ++
                                   cambodia.cache ++
                                   china.cache ++
                                   eastTimor.cache ++
                                   gCCStates.cache ++
                                   india.cache ++
                                   indonesia.cache ++
                                   iran.cache ++
                                   iraq.cache ++
                                   japan.cache ++
                                   jordan.cache ++
                                   kazakhstan.cache ++
                                   kyrgyzstan.cache ++
                                   laos.cache ++
                                   lebanon.cache ++
                                   malaysia.cache ++
                                   maldives.cache ++
                                   mongolia.cache ++
                                   myanmar.cache ++
                                   nepal.cache ++
                                   northKorea.cache ++
                                   pakistan.cache ++
                                   israelAndPalestine.cache ++
                                   philippines.cache ++
                                   southKorea.cache ++
                                   sriLanka.cache ++
                                   syria.cache ++
                                   tajikistan.cache ++
                                   thailand.cache ++
                                   turkmenistan.cache ++
                                   uzbekistan.cache ++
                                   vietnam.cache ++
                                   yemen.cache)

  }

}
