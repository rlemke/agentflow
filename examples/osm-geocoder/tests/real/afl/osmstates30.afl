namespace osm.geo.UnitedStates.sample {
  use osm.types
  use osm.geo.Operations
  use osm.geo.cache.UnitedStates

  workflow Download30States() => (cache: [OSMCache]) andThen {

    alaska = Alaska()
    arizona = Arizona()
    california = California()
    colorado = Colorado()
    connecticut = Connecticut()
    florida = Florida()
    georgia = Georgia()
    idaho = Idaho()
    illinois = Illinois()
    indiana = Indiana()
    iowa = Iowa()
    kansas = Kansas()
    kentucky = Kentucky()
    louisiana = Louisiana()
    maine = Maine()
    maryland = Maryland()
    michigan = Michigan()
    minnesota = Minnesota()
    missouri = Missouri()
    montana = Montana()
    nevada = Nevada()
    newYork = NewYork()
    northCarolina = NorthCarolina()
    ohio = Ohio()
    oregon = Oregon()
    pennsylvania = Pennsylvania()
    tennessee = Tennessee()
    texas = Texas()
    virginia = Virginia()
    washington = Washington()

    yield Download30States(cache = alaska.cache ++
                                   arizona.cache ++
                                   california.cache ++
                                   colorado.cache ++
                                   connecticut.cache ++
                                   florida.cache ++
                                   georgia.cache ++
                                   idaho.cache ++
                                   illinois.cache ++
                                   indiana.cache ++
                                   iowa.cache ++
                                   kansas.cache ++
                                   kentucky.cache ++
                                   louisiana.cache ++
                                   maine.cache ++
                                   maryland.cache ++
                                   michigan.cache ++
                                   minnesota.cache ++
                                   missouri.cache ++
                                   montana.cache ++
                                   nevada.cache ++
                                   newYork.cache ++
                                   northCarolina.cache ++
                                   ohio.cache ++
                                   oregon.cache ++
                                   pennsylvania.cache ++
                                   tennessee.cache ++
                                   texas.cache ++
                                   virginia.cache ++
                                   washington.cache)

  }

}
