namespace maven.types {

    schema ArtifactInfo {
        group_id: String,
        artifact_id: String,
        version: String,
        classifier: String,
        packaging: String,
        path: String,
        size_bytes: Long,
        checksum: String
    }

    schema DependencyTree {
        root_artifact: String,
        total_dependencies: Long,
        direct_dependencies: Long,
        transitive: Long,
        conflicts: Long,
        tree_output: String
    }

    schema BuildResult {
        artifact_path: String,
        build_tool: String,
        version: String,
        success: Boolean,
        duration_ms: Long,
        warnings: Long,
        errors: Long
    }

    schema TestReport {
        total: Long,
        passed: Long,
        failed: Long,
        skipped: Long,
        duration_ms: Long,
        report_path: String,
        coverage_pct: Double
    }

    schema PublishResult {
        repository_url: String,
        group_id: String,
        artifact_id: String,
        version: String,
        published: Boolean,
        snapshot: Boolean,
        deploy_duration_ms: Long
    }

    schema QualityReport {
        tool: String,
        issues: Long,
        critical: Long,
        major: Long,
        minor: Long,
        report_path: String,
        passed: Boolean
    }

    schema ProjectInfo {
        group_id: String,
        artifact_id: String,
        version: String,
        packaging: String,
        modules: Json,
        parent_pom: String
    }

    schema ExecutionResult {
        exit_code: Long,
        success: Boolean,
        duration_ms: Long,
        stdout: String,
        stderr: String,
        artifact_path: String
    }
}
