namespace maven.publish {
    use maven.types

    event facet DeployToRepository(artifact_path: String,
        repository_url: String,
        group_id: String, artifact_id: String,
        version: String) => (result: PublishResult)

    event facet PublishSnapshot(artifact_path: String,
        repository_url: String,
        group_id: String, artifact_id: String,
        version: String) => (result: PublishResult)

    event facet PromoteRelease(group_id: String,
        artifact_id: String,
        staging_version: String,
        release_version: String,
        repository_url: String) => (result: PublishResult)
}
